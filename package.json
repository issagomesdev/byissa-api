  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts src/migration/CreateData",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "seed": "ts-node src/seed.ts"
  },
